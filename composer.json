{
    "name": "wordplate/framework",
    "description": "The WordPlate Framework",
    "keywords": ["wordplate", "framework", "wordpress", "boilerplate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        },
        {
            "name": "Chris Andersson",
            "email": "hello@puredazzle.se"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-mbstring": "*",
        "composer/installers": "^1.0",
        "filp/whoops": "^1.1",
        "illuminate/config": "5.2.*",
        "illuminate/container": "5.2.*",
        "illuminate/support": "5.2.*",
        "johnpbloch/wordpress": "^4.0",
        "symfony/console": "3.0.*",
        "symfony/debug": "3.0.*",
        "symfony/finder": "3.0.*",
        "symfony/var-dumper": "3.0.*",
        "vlucas/phpdotenv": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "WordPlate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WordPlate\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra":{
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
