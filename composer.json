{
    "name": "wordplate/framework",
    "description": "The WordPlate Framework.",
    "keywords": ["wordplate", "wordpress", "boilerplate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        }
    ],
    "require": {
    	"php": ">=5.4.0",
        "composer/installers": "~1.0",
        "filp/whoops": "~1.0",
        "illuminate/support": "~5.0",
        "jjgrainger/wp-custom-post-type-class": "~1.0",
        "johnpbloch/wordpress": "~4.0",
        "roots/soil": "~3.0",
        "symfony/var-dumper": "~2.0",
        "vlucas/phpdotenv": "~1.0",
        "symfony/filesystem": "~3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "WordPlate\\": "src/"
        }
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ]
        },
        "wordpress-install-dir": "wordpress"
    },
    "extra":{
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
