{
    "name": "wordplate/framework",
    "description": "The WordPlate Framework",
    "keywords": ["wordplate", "framework", "wordpress", "boilerplate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "ext-mbstring": "*",
        "composer/installers": "~1.0",
        "filp/whoops": "~1.0",
        "illuminate/config": "5.1.*",
        "illuminate/container": "5.1.*",
        "illuminate/support": "5.1.*",
        "jjgrainger/wp-custom-post-type-class": "~1.0",
        "johnpbloch/wordpress": "4.2.*",
        "roots/soil": "~3.0",
        "symfony/console": "2.7.*",
        "symfony/debug": "2.7.*",
        "symfony/finder": "2.7.*",
        "symfony/var-dumper": "2.7.*",
        "vlucas/phpdotenv": "~1.0"
    },
    "require-dev": {
		"frozzare/wp-test-suite": "~1.0"
    },
    "autoload": {
        "files": [
            "src/Foundation/helpers.php"
        ],
        "psr-4": {
            "WordPlate\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "extra":{
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
