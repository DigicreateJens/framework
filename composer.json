{
    "name": "wordplate/framework",
    "description": "The WordPlate Framework",
    "keywords": ["wordplate", "framework", "wordpress", "boilerplate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        },
        {
            "name": "Chris Andersson",
            "email": "hello@puredazzle.se"
        }
    ],
    "require": {
        "ext-mbstring": "*",
        "php": ">=5.5.9",
        "composer/installers": "^1.0.29",
        "filp/whoops": "^1.1.7",
        "illuminate/config": "5.1.*",
        "illuminate/container": "5.1.*",
        "illuminate/support": "5.1.*",
        "jjgrainger/wp-custom-post-type-class": "^1.4.0",
        "johnpbloch/wordpress": "^4.0.0",
        "roots/soil": "^3.4.0",
        "symfony/console": "2.7.*",
        "symfony/debug": "2.7.*",
        "symfony/finder": "2.7.*",
        "symfony/var-dumper": "2.7.*",
        "vlucas/phpdotenv": "^1.1.1"
    },
    "require-dev": {
		"phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "files": [
            "src/Foundation/helpers.php"
        ],
        "psr-4": {
            "WordPlate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WordPlate\\Tests\\": "tests/"
        }
    },
    "extra":{
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
