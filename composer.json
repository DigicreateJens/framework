{
    "name": "wordplate/framework",
    "description": "The WordPlate framework",
    "keywords": ["wordplate", "framework", "wordpress", "boilerplate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com",
            "homepage": "https://vinkla.com"
        },
        {
            "name": "Chris Andersson",
            "email": "hello@puredazzle.se",
            "homepage": "https://puredazzle.se"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-mbstring": "*",
        "composer/installers": "^1.2",
        "illuminate/support": "5.4.*",
        "johnpbloch/wordpress": "^4.7",
        "roots/wp-password-bcrypt": "^1.0",
        "symfony/http-foundation": "^3.2",
        "symfony/var-dumper": "^3.2",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "tests/stubs/functions.php"
        ],
        "psr-4": {
            "WordPlate\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra":{
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    },
    "suggest": {
        "wordplate/mail": "A mail plugin for WordPlate",
        "wordplate/plate": "A framework plugin for WordPlate"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
